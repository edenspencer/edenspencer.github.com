---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Spencer Turner - Designer On Rails</title>
 <link href="http://designeronrails.com/eden_atom.xml" rel="self"/>
 <link href="http://designeronrails.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://designeronrails.com/</id>
 <author>
   <name>Spencer Turner</name>
   <email>spencer@designeronrails.com</email>
 </author>
 
 {% for post in site.categories.eden %}
 <entry>
   <title>{{ post.title }}</title>
   <tags>{{post.tags}}</tags>
   {% for category in post.categories %}
   <category term="{{category}}" />
   {% endfor %}
   <link href="http://designeronrails.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://designeronrails.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
